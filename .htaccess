ErrorDocument 404 /404.html

# php_value memory_limit 64M
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /mth/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /mth/index.php [L]
#RewriteRule ^login/ /mth/index.php?action=login [L]
#RewriteRule ^login /mth/index.php?action=login [L]

#RewriteRule ^register/ /mth/index.php?action=register [L]
#RewriteRule ^register /mth/index.php?action=register [L]

#RewriteRule ^about/ /mth/index.php?action=about [L]
#RewriteRule ^about /mth/index.php?action=about [L]

#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
</IfModule>
# BEGIN WordPress